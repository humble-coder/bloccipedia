<h1><%= markdown @wiki.title %></h1>
<p><%= markdown @wiki.body %></p>
<% if @wiki.users.include?(current_user) %>
  <button><%= link_to "Edit Wiki", edit_wiki_path(@wiki) %></button>
  <h3>Collaborators</h3>
  <% @wiki.users.each do |user| %>
    <p><%= user.name %> 
    <% if current_user.wikis.include?(@wiki) %>
      <%= link_to "Remove " + user.name, wiki_user_path(@wiki, user), method: :delete %></p>
    <% end %>
  <% end %>
<% end %>
